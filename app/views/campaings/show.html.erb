<%- model_class = Campaing -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:campaingName) %>:</strong></dt>
  <dd><%= @campaing.campaingName %></dd>
  <dt><strong><%= model_class.human_attribute_name(:campaingCode) %>:</strong></dt>
  <dd><%= @campaing.campaingCode %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @campaing.status %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              campaings_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_campaing_path(@campaing), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              campaing_path(@campaing),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
